<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:textStyle="bold"
        android:id="@+id/item_profile_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="identificationProfileId" />

    <TextView
        android:id="@+id/item_enroll_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="enrollmentSpeechTime" />

    <TextView
        android:id="@+id/item_remain_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="remainingEnrollmentSpeechTime" />

    <TextView
        android:id="@+id/item_status"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="enrollmentStatus" />
</LinearLayout>